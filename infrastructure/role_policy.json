{
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "kms:GetSecretValue"
            ],
            "Resource": "acs:kms:*:*:secret/oos/leapp-bin/*"
        },
        {
            "Action": [
                "oos:ListParameterVersions",
                "oos:UpdateParameter"
            ],
            "Effect": "Allow",
            "Resource": "acs:oos:*:*:parameter/leapp-bin/*"
        },
        {
            "Action": [
                "oos:GetSecretParameter"
            ],
            "Effect": "Allow",
            "Resource": "acs:oos:*:*:secretparameter/leapp-bin/*"
        }        
    ],
    "Version": "1"
}
